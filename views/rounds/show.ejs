<% layout('layouts/boilerplate') -%>
<h2><%= operative %></h2>

<div class="table-responsive">
    <table class="table">
        <thead>
        <tr>
            <th>Round</th>
            <th>Ref.</th>
            <th>Name</th>
            <th>Address</th>
            <th>Details</th>
            <th>Freq.</th>
            <th>Ext</th>
            <th>Int</th>
            <th>Acc/Manager</th>

        </tr>
    </thead>
        <% for (let job of round) { %>
            <tr>
                <td>
                    <%= job.roundNumber %>
                </td>
                <td>
                    <%= job.ref %>
                </td>
                <td>
                    <%= job.name %>
                </td>
                <td>
                    <%= job.address %>
                </td>
                <td>
                    <%= job.details %>
                </td>
                <td>
                    <%= job.freq %>
                </td>
                <td>
                    <%= job.exterior %>
                </td>
                <td>
                    <%= job.interior %>
                </td>
                <td>
                    <%= job.accManager %>
                </td>
                <td>
                    <a class="btn btn-dark" href="/jobs/<%= job._id %> ">View</a>
                </td>
            </tr>
            <% } %>

    </table>
 




